ifconfig:

name: eth0  
    inet_addr: 11.0.0.2
    Mask: 255.255.255.0
    MTU: 1500
    Metric: 1
    POINTTOPOINT MULTICAST
    
name: wlan0
    inet_addr: 192.168.0.100
    Mask: 255.255.255.0
    MTU: 1500
    Metric: 1
    BROADCAST MULTICAST

ifconfigend.

route:

#Destination     Gateway          Genmask          Flags  Metric  Iface
192.168.2.0      *                255.255.255.0    H      0       eth0
11.0.0.0         *                255.255.255.0    H      0       eth0
192.168.0.0      *                255.255.255.0    H      0       wlan0
192.168.3.0      *                255.255.255.0    H      0       wlan0
# saida default
0.0.0.0          *                0.0.0.0          G      0       eth0


routeend.

