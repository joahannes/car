ifconfig:

# interface to the external client
name: eth0  
    inet_addr: 192.168.2.1
    Mask: 255.255.255.0
    MTU: 1500
    Metric: 1
    POINTTOPOINT MULTICAST
    
# Interface Wi-Fi (wlan0) â€“ sub-rede 192.168.0.0/24
name: wlan0
    inet_addr: 192.168.0.1
    Mask: 255.255.255.0
    MTU: 1500
    Metric: 1
    BROADCAST MULTICAST
    
name: cellular
    inet_addr: 10.0.0.1
    Mask: 255.255.255.0
    MTU: 1500
    POINTTOPOINT MULTICAST
    
ifconfigend.

route:
#Destination     Gateway          Genmask          Flags  Metric  Iface
192.168.3.0      *                255.255.255.0    H      0       wlan0
192.168.3.0      *                255.255.255.0    H      100       cellular
192.168.2.0      *                255.255.255.0    H      0       eth0
192.168.100.0    *                255.255.255.0    H      0       eth0
10.0.100.0       *                255.255.255.0    H      0       eth0
192.168.0.0      *                255.255.255.0    H      0       wlan0
0.0.0.0          *                0.0.0.0          G      0       cellular

routeend.

